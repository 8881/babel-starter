{
  "name": "babel-starter",
  "version": "1.0.0",
  "description": "一个可以自由使用 es2015/es7 的 CommonJS 环境",
  "main": "build/index.js",
  "keywords": [
    "babel",
    "es2015",
    "es6",
    "es7",
    "CommonJs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/8881/babel-starter.git"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "babel-node ./src/index.js",
    "dev": "npm run clean && babel ./src --out-dir ./build --watch",
    "build": "npm run clean && babel ./src --out-dir ./build"
  },
  "author": "8881",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0"
  }
}
